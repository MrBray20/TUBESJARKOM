<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Hyperlink?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.clientjarkom.PrimaryController">
   <children>
      <Label text="Login" />
      <Label text="Username"/>
      <TextField fx:id="usernameField"/>
      <Label text="Password"/>
      <PasswordField fx:id="passwordField"/>
      <Button text="Login" onAction="#loginButton"/>
      <Hyperlink text="Belum punya akun? klik di sini untuk register." onAction="#goToRegister"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
